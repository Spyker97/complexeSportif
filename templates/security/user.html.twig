{% extends 'base.user.html.twig' %}

{% block afficher %}

    {% for user in users %}
    <tr>

        <td>{{ user.cin  }} </td>
        <td> {{ user.username }}</td>
        <td> {{ user.prenom }}</td>
        <td> {{ user.genre }}</td>
        <td> {{ user.getDateNaissance() |date('Y-m-d') }} </td>
        <td> {{ user.email }}</td>
        <td> {{ user.password }}</td>
        <td> {{ user.adresse }}</td>
        <td>
            <a href="/user/delete/ {{ user.cin }}">Delete</a>
            <a href="#">Edite</a>
        </td>

    </tr>
    {% endfor %}
{% endblock %}